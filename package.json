    "formik": "^2.2.9",
    "framer-motion": "^7.6.4",
    "fs-extra": "^10.1.0",
    "next": "13.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "winston": "^3.8.2",
