{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "PhoneNumbers",
  "properties": {
    "records": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "number": {
            "type": "string"
          }
        },
        "required": ["name", "number"]
      }
    }
  },

  "required": ["records"]
}
